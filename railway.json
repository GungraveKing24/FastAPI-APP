{
  "services": [
    {
      "name": "backend",
      "env": "python",
      "rootDirectory": "backend",
      "buildCommand": "pip install -r requirements.txt",
      "startCommand": "bash start.sh"
    },
    {
      "name": "frontend",
      "env": "node",
      "rootDirectory": "frontend",
      "buildCommand": "npm install && npm run build",
      "startCommand": "npx serve -s dist"
    }
  ]
}
